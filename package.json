{
  "name": "react-ts-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc -b && pnpm vite build --mode dev",
    "build:test": "tsc -b && pnpm vite build --mode test",
    "build:pro": "tsc -b && pnpm vite build --mode pro",
    "preview:dev": "pnpm vite preview --mode dev",
    "preview:test": "pnpm vite preview --mode test",
    "preview:pro": "pnpm vite preview --mode pro",
    "format": "prettier --write \"./**/*.{html,ts,tsx,js,jsx,json}\"",
    "lint": "eslint .",
    "lint:style": "stylelint \"./**/*.{css,scss,html}\" --fix",
    "preinstall": "npx only-allow pnpm",
    "push": "git add . && git commit -m \"feat: update\" && git push -u origin master"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.8.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "sass": "^1.80.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9"
  }
}